<!DOCTYPE html>
<html>
  <head>
    <%- include('../head') %>
  </head>
  <body>
    <%- include('../nav') %>
    <div class="container mb-3">
      <h3 class="contentBoxTop"><%= session.memberID %></h3>
        <fieldset disabled>
          <div class="form-group row">
            <label for="name" class="col-sm-3 col-form-label">Name</label>
            <div class="col-sm-9">
              <input class="form-control" type="text" id="name" name="name" value= <%=session.name %>>
            </div>
          </div>
          <div class="form-group row">
            <label for="email" class="col-sm-3 col-form-label">Email</label>
            <div class="col-sm-9">
              <input class="form-control" type="text" id="email" name="email" value= <%= session.email %>>
            </div>
          </div>
        </fieldset>
      <div>
          <a class="btn btn-primary" href="/User/Edit">Edit</a>
      </div>

    </div>
  </body>
</html>
